# Ignore entire target directory
target/

# But do NOT ignore JAR files inside target
!target/*.jar

# Maven Wrapper
!.mvn/wrapper/maven-wrapper.jar

# Allow target folders inside source trees
!**/src/main/**/target/
!**/src/test/**/target/

